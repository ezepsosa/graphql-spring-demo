type Book{
    id: ID
    title: String
    publicationYear: Int
    authors: [Author]
}

type Author{
    id: ID
    name: String
    nationality: String
    books: [Book]
}

type Query{
    books: [Book]
    authors: [Author]
}

input CreateBookInput {
  title: String!
  publicationYear: Int!
  authorIds: [ID]!
}

type Mutation {
  createBook(input: CreateBookInput!): Book
}